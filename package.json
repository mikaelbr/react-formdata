{
  "name": "react-formdata",
  "version": "2.0.0",
  "description": "A library for generating an object of values from a set of inputs in React",
  "main": "lib/formdata.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register  --require ./tests/test-bootstrap.js tests/*-test.js",
    "prebuild": "rm -rf lib",
    "build": "babel src --out-dir lib",
    "docs": "jsdoc2md src/formdata.js",
    "lint": "eslint src/formdata.js example/demo.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "form data",
    "form"
  ],
  "author": "Mikael Brevik <mikaelbre@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta4",
    "babel-plugin-transform-react-jsx": "^6.2.0",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-react": "^3.11.2",
    "jsdoc-to-markdown": "^1.2.1",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4"
  },
  "peerDependencies": {
    "react": ">=15.0.0"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
